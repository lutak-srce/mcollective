topicprefix = /topic/
main_collective = mcollective
collectives = mcollective
libdir = /usr/libexec/mcollective
logfile = /var/log/mcollective.log
loglevel = debug
daemonize = 1

# Plugins
securityprovider = psk
plugin.psk = unset

connector = stomp
plugin.stomp.host = <%= @mqmaster %>
plugin.stomp.port = <%= @mqport %> #port for rabbitmq
plugin.stomp.user = <%= @mquser %>
plugin.stomp.password = <%= @mqpassword %>

# Facts
factsource = yaml
plugin.yaml = /etc/mcollective/facts.yaml
